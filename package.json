{
  "name": "@basestack/vercel-flags-sdk-adapter",
  "version": "1.0.0",
  "description": "Vercel Flags adapter backed by the Basestack Feature Flags API.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "node_modules && rm -rf dist",
    "update:deps": "bun update -i",
    "lint": "bunx biome lint .",
    "format": "bunx biome format --write .",
    "check": "bunx biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepublishOnly": "bun run lint && bun run test && bun run build"
  },
  "keywords": [
    "vercel",
    "flags",
    "feature-flags",
    "basestack",
    "adapter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basestack-co/basestack-vercel-flags-sdk-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/basestack-co/basestack-vercel-flags-sdk-adapter/issues"
  },
  "homepage": "https://github.com/basestack-co/basestack-vercel-flags-sdk-adapter#readme",
  "engines": {
    "node": ">=18.17.0"
  },
  "packageManager": "bun@1.3.2",
  "dependencies": {
    "flags": "^4.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@types/bun": "^1.3.2",
    "@types/node": "^24.10.1",
    "tsdown": "^0.16.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
